        <section name="scriptor">
            <section name="body">
                <value><![CDATA[import java.util.List;
import com.wfp.utils.*;
import com.wfp.security.form.DeviceBean;

private com.enterprisehorizons.magma.designtime.artifact.GeoArtifact $artefact;
private com.enterprisehorizons.magma.datamashup.ScriptableDataDriver $customdriver;
private com.enterprisehorizons.magma.datamashup.SimpleDataDriver $simplemashupdriver;
int lat =24;
Map<String,String> missionsMap = LDAPUtils.getMissionsMap();
List<String> userMissionsList = null;

void bindData() {   
	  
     DeviceBean data= (DeviceBean)$customdatamodel;
   
    if(data!=null ) { $artefact.setCoordinates(data.getCoordStr());
    				  String cord[]= data.getCoordStr().split(",");
    				  $artefact.addAttribute("longitude",cord[0]);
    				  $artefact.addAttribute("latitude",cord[1]);
    				  
    				  
			//changed for getting type as Date instead of String for time
			//$artefact.addAttribute("time", com.wfp.utils.CommonUtils.parseDatePortalFormat(data.getTime().replace("T", " ")));	
			Date t = (Date) com.wfp.utils.CommonUtils.stringDateToDate(data.getTime().replace("T"," ").replace(".000Z","" ) );	
			$artefact.addAttribute("time",	t );
			
			//$artefact.addAttribute("localTime", com.wfp.utils.CommonUtils.stringDateToDate(data.getDeviceLocalTime()) );		
			//$artefact.addAttribute("localTimeText", data.getDeviceLocalTime() );   
			
			$artefact.addAttribute("name", data.getName());  
			$artefact.addAttribute("Organization", data.getOrganization());
			$artefact.addAttribute("title", data.getTitle());
			$artefact.addAttribute("owner", getFullName(data));
			$artefact.addAttribute("IM", com.wfp.utils.CommonUtils.getPagerDtls(data.getSkypePager()));
			$artefact.addAttribute("Mail", com.wfp.utils.CommonUtils.getMailDtls(data.getMail()));
			$artefact.addAttribute("Phone", com.wfp.utils.CommonUtils.getPhoneDtls(data.getMobilesList()));
			$artefact.addAttribute("LatitudeStr", cord[1]+" / "+cord[0] );
			$artefact.addAttribute("QRcode", "QRcode");
			$artefact.addAttribute("Occured",  com.wfp.utils.DHLUtils.getHumanReadable( t ) );
		
		} 
}
String getFullName(com.wfp.security.form.DeviceBean data){
	String fullName = "";
	if(!com.enterprisehorizons.util.StringUtils.isNull(data.getCn())){
		fullName = data.getCn()+" ";
	}
	
	if(!com.enterprisehorizons.util.StringUtils.isNull(data.getSn())){
		fullName += data.getSn();
	}
	
	return fullName;
}

List getData() {
Object deviceId = getSession().getHttpSession().getAttribute("session_deviceId");
if( deviceId==null ) deviceId="trackMe-1";
return LDAPWSUtils.getUnitHistorical( deviceId.toString(), "staff" ); 
  
}

List<String> getUserMissionList( String userId )
{
	if(userMissionsList==null )
	{
		userMissionsList= LDAPUtils.getAllMisisons4rUser( userId );
	} //System.out.println("userMissionsList"+ userMissionsList );
	return userMissionsList;
}
String getNameStyle() {
    if("Meter1".equalsIgnoreCase($artefact.getAttributeValueAsString("Meter1")))
	return "EsriStyle1";
	else
	return "EsriStyle2";
	
}

String getName() { 	return $artefact.getAttributeValueAsString("Name"); }
String getLAbelName() { 	return $artefact.getAttributeValueAsString("Name") +" - "+ $artefact.getAttributeValueAsString("time"); }
boolean authorize(){

String type = $artefact.getAttributeValueAsString("Name");
if( type!=""&& type.equalsIgnoreCase("trackMe-7") ) return true;
return false;
}
String getIconUrl(){
	return "serverresources/icons/epic/Tracking_Staff_G.png";
}

String getIconColor(){
	return "#00FF00";
}

String getLineColor(){
	try{
	com.wfp.security.form.DeviceBean data = (com.wfp.security.form.DeviceBean)$customdatamodel;
	String deviceId =getName();
		
	if(com.enterprisehorizons.magma.server.util.Cache.contains("$colorCache$")){
		Map map = (HashMap) com.enterprisehorizons.magma.server.util.Cache.retrieve("$colorCache$");
	 	String color = (String) map.get(deviceId);
	 	
	 	if(color == null){ 					
			color =  "#"+com.enterprisehorizons.util.ColorUtils.getRandomColor();
			while(color.length() != 7){
				color =  "#"+com.enterprisehorizons.util.ColorUtils.getRandomColor();
			}
			map.put(deviceId, color);
	 	}
	 	return color;
	 }
	 }catch(Exception e){
	 	//e.printStackTrace();
	 }
 
 return "#FF0000";
